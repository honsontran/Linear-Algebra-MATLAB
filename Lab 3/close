H = house; plot2d(H), hold on
r = .9; D = [r, 0; 0, 1/r ]
D =
    0.9000         0
         0    1.1111
plot2d(D*H)
r = .8; D = [r, 0; 0, 1/r ]
D =
    0.8000         0
         0    1.2500
